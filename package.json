{
    "name": "winwoga-trans-node",
    "version": "0.1.0",
    "description": "A simple Node app built on fastify, instantly up and running. Built to be remixed on Glitch.com.",
    "main": "server.js",
    "scripts": {
      "start": "node server.js", 
      "prebuild": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
      "build": "cd android && ./gradlew assembleDebug && cd../"
  
    },
    "dependencies": {
      "fastify": "^4.21.0",
      "handlebars": "^4.7.8",
      "@fastify/formbody": "^7.4.0",
      "@fastify/static": "^6.10.2",
      "@fastify/view": "^8.0.0",
      "axios": "^1.7.7",
      "bcrypt": "^6.0.0",
      "cors": "^2.8.5",
      "express": "^5.1.0",
      "https": "^1.0.0",
      "jsonwebtoken": "^9.0.2",
      "mongodb": "^6.16.0",
      "mongoose": "^8.13.2",
      "socket.io": "^4.8.1"
    },
    "engines": {
      "node": "20.x"
    },
    "repository": {
      "url": "https://glitch.com/edit/#!/glitch-hello-node"
    },
    "license": "MIT",
    "keywords": [
      "node",
      "glitch"
    ]
  }
  